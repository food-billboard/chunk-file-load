{
  "name": "chunk-file-upload",
  "version": "1.0.8",
  "description": "文件分片上传",
  "private": false,
  "main": "./lib/index.js",
  "typings": "./lib/index.d.ts",
  "types": "./lib/index.d.ts",
  "homepage": "https://food-billboard.github.io/chunk-file-load/",
  "scripts": {
    "prepare": "husky install",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable",
    "server": "node ./mock_data/server/index.js",
    "start": "dumi dev",
    "build": "npm run build:lib && npm run build:es && npm run build:umd",
    "build:lib": "cross-env BUILD_TYPE=lib father-build",
    "build:umd": "cross-env BUILD_TYPE=umd father-build",
    "build:es": "cross-env BUILD_TYPE=es father-build",
    "docs:build": "dumi build",
    "docs:deploy": "gh-pages -d docs-dist",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\"",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage",
    "deploy": "np"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "files": [
    "es/",
    "lib/",
    "dist/"
  ],
  "keywords": [
    "chunk",
    "upload",
    "file",
    "utils"
  ],
  "author": "glf",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/food-billboard/chunk-file-load"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "@babel/runtime": "^7.15.4",
    "eventemitter3": "^4.0.7",
    "lodash": "^4.17.21",
    "spark-md5": "^3.0.1"
  },
  "devDependencies": {
    "@ant-design/icons": "^4.6.4",
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-angular": "^13.1.0",
    "@koa/router": "^10.1.1",
    "@types/jest": "^26.0.14",
    "@types/lodash": "^4.14.168",
    "@types/spark-md5": "^3.0.2",
    "@umijs/test": "^3.0.5",
    "antd": "^4.16.13",
    "axios": "^0.21.4",
    "babel-jest": "^26.3.0",
    "babel-plugin-import": "^1.13.3",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "dumi": "^1.0.16",
    "father-build": "^1.17.2",
    "gh-pages": "^3.2.3",
    "husky": "^7.0.2",
    "jest": "^26.4.2",
    "koa": "^2.13.3",
    "koa-cors": "^0.0.16",
    "koa2-multiparty": "^1.0.1",
    "lint-staged": "^10.0.7",
    "pinst": "^2.1.6",
    "prettier": "^2.2.1",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "ts-jest": "^26.4.0",
    "yorkie": "^2.0.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
